    "publish:beta:continue": "lerna publish from-package --dist-tag beta --force-publish --ignore-scripts",
    "publish:beta:patch": "lerna publish prepatch --dist-tag beta --preid beta --force-publish --ignore-scripts",
    "publish:beta:minor": "lerna publish preminor --dist-tag beta --preid beta --force-publish --ignore-scripts",
    "publish:beta:prerelease": "lerna publish prerelease --dist-tag beta --preid beta --force-publish --ignore-scripts",
    "postinstall": "lerna bootstrap && git config --local include.path ../.gitconfig",
    "clean-install": "git clean -xdf . && yarn install && lerna bootstrap && ./scripts/build babel",
